@page
@model ConorWebsite.Pages.AboutModel
@{
    ViewData["Title"] = "About";
}

<div class="text-center">
    <h1 class="display-4 font-weight-bold text-light">About</h1>
    <div class="alert alert-warning alert-dismissible fade show my-5" role="alert">
        Welcome to the <strong>@ViewData["Title"] Page</strong>.
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
</div>